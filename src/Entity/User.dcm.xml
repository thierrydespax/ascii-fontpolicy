<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="User" table="user">
    <indexes>
      <index name="user_level_id" columns="user_level_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="user_mail" columns="user_mail"/>
    </unique-constraints>
    <id name="userId" type="integer" column="user_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userMail" type="string" column="user_mail" length="128" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="userPswd" type="string" column="user_pswd" length="128" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <many-to-one field="userLevel" target-entity="UserLevel" fetch="LAZY">
      <join-columns>
        <join-column name="user_level_id" referenced-column-name="user_level_id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
