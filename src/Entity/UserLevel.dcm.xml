<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UserLevel" table="user_level">
    <unique-constraints>
      <unique-constraint name="user_level_name" columns="user_level_name"/>
    </unique-constraints>
    <id name="userLevelId" type="integer" column="user_level_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userLevelName" type="string" column="user_level_name" length="32" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
